  keywords = {3' Untranslated Regions; Animals; Chromatin Immunoprecipitation; CpG
        Islands; Evolution, Molecular; Genome; Genome, Human; Humans; Mice;
        Promoter Regions, Genetic; RNA Polymerase II, genetics; TATA Box;
        Transcription Initiation Site},
  language = {eng},
